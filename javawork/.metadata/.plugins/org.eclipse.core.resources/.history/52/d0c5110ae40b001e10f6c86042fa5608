package day0616;

public class ParseNum_02 {
	public static void main(String[] args) {
		
		// String -> int 형변환 
		// Integer.parseInt 
		// Double.parseDouble("12.23");
		
		String num1 ="10";
		String num2 ="20";
		
		
		System.out.println("두 수 더하기 : "+Integer.parseInt(num1)+Integer.parseInt(num2));
		
	}
}
